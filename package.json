    "@types/mocha": "^5.0.0",
    "@types/node": "<=9.6.13",
    "chai": "^4.1.2",
    "chai-events": "^0.0.1",
    "mocha": "^5.1.0",
    "nyc": "^11.6.0",
    "prettier": "^1.12.1",
